# loggerConfig.cmake.in
# 用于 find_package(logger) 的配置文件

@PACKAGE_INIT@

# 包含生成的导出目标文件
include("${CMAKE_CURRENT_LIST_DIR}/loggerTargets.cmake")

# 设置友好变量（可选）
set(logger_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@/logger")
